{
  "manifest_version": 3,
  "name": "KI-Assistent",
  "version": "0.1",
  "description": "Fasst Inhalt zusammen und bewertet ihn qualitativ",
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": "ai.png",
    "default_title": "GO"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}
